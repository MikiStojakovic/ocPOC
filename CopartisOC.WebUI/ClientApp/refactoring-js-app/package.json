{
  "name": "refactoring-js",
  "version": "1.0.0",
  "description": "refactoring exerice based on Martin Fowler Refactoring 2nd edition",
  "main": "main.js",
  "scripts": {
    "test": "jest",
    "start": "babel src --out-dir dist"
  },
  "keywords": [
    "js",
    "refactoring",
    "jest"
  ],
  "author": "Miroslav Stojakovic",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "babel-jest": "^26.0.1",
    "babel-polyfill": "^6.26.0",
    "jest": "^26.0.1"
  },
  "jest": {
    "testRegex": "src/.*\\.test\\.(js|jsx)$",
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  }
}
